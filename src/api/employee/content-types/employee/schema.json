{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "Employee"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "employee_id": {
      "type": "uid"
    },
    "employee_first_name": {
      "type": "string"
    },
    "employee_last_name": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "date_of_birth": {
      "type": "date"
    },
    "date_joined": {
      "type": "date"
    },
    "date_left": {
      "type": "date"
    },
    "employee_address": {
      "type": "string"
    },
    "marriage_status": {
      "type": "boolean"
    },
    "employee_remark": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "employee"
    },
    "employee_job_assignments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employee-job-assignment.employee-job-assignment",
      "mappedBy": "employee_id"
    },
    "performence_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::performence-review.performence-review",
      "mappedBy": "employee_id"
    },
    "leaves": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::leave.leave",
      "mappedBy": "employee"
    }
  }
}
